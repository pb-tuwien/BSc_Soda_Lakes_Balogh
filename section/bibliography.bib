@article{danielsenApplicationTransientElectromagnetic2003,
  title = {The Application of the Transient Electromagnetic Method in Hydrogeophysical Surveys},
  author = {Danielsen, Jens E. and Auken, Esben and JÃ¸rgensen, Flemming and SÃ¸ndergaard, Verner and SÃ¸rensen, Kurt I.},
