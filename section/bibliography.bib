@article{galazoulasLargeScaleElectrical2015,
  title = {Large {{Scale Electrical Resistivity Tomography Survey Correlated}} to {{Hydrogeological Data}} for {{Mapping Groundwater Salinization}}: {{A Case Study}} from a {{Multilayered Coastal Aquifer}} in {{Rhodope}}, {{Northeastern Greece}}},
